,MAAI,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,iBAAE,QAAA,CAAA,kBAAA,IAAA,QAAA,CAAA,kBAAA,OAAA,QAAA,YAAF;AAAA,SAAA;AACL,OALF,E,IAAA,E;;AAAA,O,iBAGW,KAAA,CAAA,QAAA,CAAS,K,GAIhB,E,CADM,KAAA,CAAA,QAAA,CAAS,O,kBAAnB,YAAA,CAA6D,GAA7D,EAA6D,WAA7D,EAA4B,+BAA5B,C,qCACI,CATN,C,EAUA,YAAA,CASM,KATN,EAAA,WAAA,EASM,CARJ,WAQI,E,gBAPJ,YAAA,CAKE,OALF,EAKE;AAJA,QAAA,IAAI,EAAC,UAIL;AAHA,QAAA,EAAE,EAAC,SAGH;;iBAFS,KAAA,CAAA,OAAA,CAAQ,KAAR,GAAa,M;UAEtB;AADC,QAAA,MAAI,EAAA,MAAA,CAAA,EAAA,CAAA,KAAA,MAAA,CAAA,EAAA,CAAA,GAAA;AAAA,iBAAE,QAAA,CAAA,iBAAA,IAAA,QAAA,CAAA,iBAAA,OAAA,QAAA,YAAF;AAAA,SAAA;AACL,OALF,E,IAAA,E;;AAAA,O,iBAGW,KAAA,CAAA,OAAA,CAAQ,K,GAIf,E,CADM,KAAA,CAAA,OAAA,CAAQ,O,kBAAlB,YAAA,CAA0D,GAA1D,EAA0D,WAA1D,EAA2B,6BAA3B,C,qCACI,CATN,C,EAWA,YAAA,CAAmC,sBAAnC,EAAmC,IAAnC,EAAmC;yBAAtB;AAAA,iBAAQ,C,WAAA,CAAR;AAAA,S,CAAsB;;;;AAAA,OAAnC,C,CAtDF,E;;AAAA,OAuDO,CAvDP;AAAA,K,CAwDU;;;;AAAA,GAzDZ,C","sourcesContent":["<template>\n  <base-dialog :show=\"isLoading\" fixed title=\"Authenticating...\">\n    <base-spinner></base-spinner>\n  </base-dialog>\n\n  <base-card>\n    <form @submit.prevent=\"formSubmit\">\n      <div class=\"form-control\">\n        <label for=\"name\">Name</label>\n        <input\n          type=\"text\"\n          id=\"name\"\n          v-model.trim=\"name.value\"\n          @blur=\"nameValidation\"\n        />\n        <p v-if=\"!name.isValid\">Please enter your name</p>\n      </div>\n\n      <div class=\"form-control\">\n        <label for=\"surname\">Surname</label>\n        <input\n          type=\"text\"\n          id=\"surname\"\n          v-model.trim=\"surname.value\"\n          @blur=\"surnameValidation\"\n        />\n        <p v-if=\"!surname.isValid\">Please enter your surname</p>\n      </div>\n\n      <div class=\"form-control\">\n        <label for=\"email\">E-Mail</label>\n        <input\n          type=\"email\"\n          id=\"email\"\n          v-model.trim=\"email.value\"\n          @blur=\"emailValidation\"\n        />\n        <p v-if=\"!email.isValid\">Please enter a valid email</p>\n      </div>\n      <div class=\"form-control\">\n        <label for=\"password\">Password</label>\n        <input\n          type=\"password\"\n          id=\"password\"\n          v-model=\"password.value\"\n          @blur=\"passwordValidation\"\n        />\n        <p v-if=\"!password.isValid\">Please enter a valid password</p>\n      </div>\n      <div class=\"form-control\">\n        <label for=\"confirm\">Confirm Password</label>\n        <input\n          type=\"password\"\n          id=\"confirm\"\n          v-model=\"confirm.value\"\n          @blur=\"confirmValidation\"\n        />\n        <p v-if=\"!confirm.isValid\">The password doesn't match!</p>\n      </div>\n\n      <base-button>Register</base-button>\n    </form>\n  </base-card>\n</template>\n\n\n\n\n\n<script>\nexport default {\n  data() {\n    return {\n      name: {\n        value: \"\",\n        isValid: true,\n      },\n      surname: {\n        value: \"\",\n        isValid: true,\n      },\n      email: {\n        value: \"\",\n        isValid: true,\n      },\n      password: {\n        value: \"\",\n        isValid: true,\n      },\n      confirm: {\n        value: \"\",\n        isValid: true,\n      },\n      isFormValid: true,\n      isLoading: false,\n    };\n  },\n  methods: {\n    formSubmit() {\n      this.nameValidation();\n      this.surnameValidation();\n      this.emailValidation();\n      this.passwordValidation();\n      this.confirmValidation();\n      if (!this.isFormValid) {\n        return;\n      }\n      this.isLoading = true;\n\n      // do the post\n\n\n\n      setInterval(() => {\n          this.isLoading = false;\n      }, 2000);\n    },\n\n    nameValidation() {\n      this.isFormValid = true;\n      this.name.isValid = true;\n      if (this.name.value === \"\") {\n        this.isFormValid = false;\n        this.name.isValid = false;\n      }\n    },\n    surnameValidation() {\n      this.isFormValid = true;\n      this.surname.isValid = true;\n      if (this.surname.value === \"\") {\n        this.isFormValid = false;\n        this.surname.isValid = false;\n      }\n    },\n\n    emailValidation() {\n      this.email.isValid = true;\n      this.isFormValid = true;\n      if (!this.email.value || !this.email.value.includes(\"@\")) {\n        this.email.isValid = false;\n        this.isFormValid = false;\n      }\n    },\n    passwordValidation() {\n      this.isFormValid = true;\n      this.password.isValid = true;\n      if (!this.password.value || this.password.value.length < 4) {\n        this.isFormValid = false;\n        this.password.isValid = false;\n      }\n    },\n    confirmValidation() {\n      this.isFormValid = true;\n      this.confirm.isValid = true;\n      if (\n        !this.confirm.value ||\n        this.confirm.value < 4 ||\n        this.confirm.value !== this.password.value\n      ) {\n        this.isFormValid = false;\n        this.confirm.isValid = false;\n      }\n    },\n  },\n};\n</script>\n\n\n\n\n\n\n\n<style scoped>\nform {\n  margin: 1rem;\n  border: 1px solid #ccc;\n\n  padding: 1rem;\n}\n\n.form-control {\n  margin: 0.5rem 0;\n}\n\nlabel {\n  font-weight: bold;\n  margin-bottom: 0.5rem;\n  display: block;\n}\n\ninput,\ntextarea {\n  display: block;\n  width: 100%;\n  font: inherit;\n  border: 1px solid #ccc;\n  padding: 0.15rem;\n}\n\ninput:focus,\ntextarea:focus {\n  border-color: #3d008d;\n  background-color: #faf6ff;\n  outline: none;\n}\n</style>"],"sourceRoot":""}]}